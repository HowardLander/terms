{
  "@context": "https://raw.githubusercontent.com/nqueder/terms/master/context/cde_context.jsonld",
  "@type": "DataElement",
  "description": "Which session was used for the validation set. For participants with two sessions, the other session was used for exploration.",
  "source_variable": "validation_session",
  "levels": [
    "1:The first session was used for validation (if present, the second session was used for exploration)",
    "2:The second session was used for validation (the first session was used for exploration)",
    "n/a:The data was not used for the (validation) analyses"
  ],
  "allowableValues": [
    "1",
    "2",
    "n/a"
  ],
  "valueType": "xsd:float"
}